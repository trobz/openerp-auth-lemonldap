<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">
		
        <record id="auth_lemonldap_enabled" model="ir.config_parameter">
        	<field name="key">auth_lemonldap.enabled</field>
        	<field name="value">False</field>
        </record>

        <record id="auth_lemonldap_secret" model="ir.config_parameter">
        	<field name="key">auth_lemonldap.secret</field>
